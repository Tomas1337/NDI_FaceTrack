    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Preset Camera Configuration",
            "type": "python",
            "request": "launch",
            "program": "FaceTrackInterface.py",
            "console": "integratedTerminal",
            "args": ["--name","DESKTOP-C16VMFB (VLC)","--id", "2"], 
            "python": "${command:python.interpreterPath}"
            
        },
        {
            "name": "Stand-Alone",
            "type": "python",
            "request": "launch",
            "program": "FaceTrackInterface.py",
            "console": "integratedTerminal",
            "python": "${command:python.interpreterPath}"
        },
        {
            "name": "Hidden UI",
            "type": "python",
            "request": "launch",
            "program": "FaceTrackInterface.py",
            "console": "integratedTerminal",
            "args": ["--name","DESKTOP-C16VMFB (VLC)","--enable_console", "False"], 
            //"args": ["--enable_console", "False"], 
            "python": "${command:python.interpreterPath}"
            
        },
        {
            "name": "Logging Test",
            "type": "python",
            "request": "launch",
            "program": "FaceTrackInterface.py",
            "console": "integratedTerminal",
            "args": ["--name","DESKTOP-C16VMFB (VLC)","--enable_console", "True", "--logging", "True"], 
            //"args": ["--enable_console", "False", "--logging", "True"], 
            "python": "${command:python.interpreterPath}"
            
        },
    ]
}