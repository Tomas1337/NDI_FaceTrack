    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "TrackingServer.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "1"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true
        },
        {
            "name": "FaceTrackInterface",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/FaceTrackInterface.py",
            "console": "externalTerminal"
        }
    ],
    "compounds": [
        {
            "name": "Server/Client",
            "configurations": ["Python: Flask", "FaceTrackInterface"]
        }
    ]
}